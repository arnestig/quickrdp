										<enabled>0</enabled>
										<tooltip>View properties</tooltip>
										<handler function="OnEditButtonClick" entry="EVT_BUTTON" />
									</object>
									<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
									<border>5</border>
								</object>
